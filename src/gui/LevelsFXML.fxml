<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import java.net.*?>
<?import javafx.scene.*?>
<!--<?import javafx.embed.swing.*?>  -->
<?import javafx.scene.media.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.effect.Light?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>


<GridPane id="navigation" fx:id="rootPane" alignment="CENTER" gridLinesVisible="false" hgap="60.0" vgap="80.0" prefHeight="900.0" prefWidth="1400.0" 
          xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FXMLLevelsController">
    <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
    <children>
        <!--Ãœberschrift-->
        <HBox alignment="CENTER" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="3">
            <Label text="Light  ON" textFill="#b2ac3b">
                <font>
                    <Font name="Berlin Sans FB Demi Bold" size="112.0" />
                </font>
                <effect>
                    <Lighting bumpInput="$null" diffuseConstant="2.0" specularConstant="2.0" specularExponent="10.23" surfaceScale="2.31">
                        <light>
                            <Light.Distant azimuth="271.05" color="#fbff00" elevation="53.37" />
                        </light>
                    </Lighting>
                </effect>
            </Label>
        </HBox>
         <!--LevelButtons von Challenge-->
        <VBox alignment="CENTER" spacing="20" GridPane.columnIndex="0" GridPane.rowIndex="1" style="-fx-background-color: transparent">
            <HBox alignment="CENTER" spacing="15">
                <Button  id="level1" mnemonicParsing="false" onAction="#level1" alignment="CENTER" prefHeight="150.0" prefWidth="150.0"  />
                <Button  id="level2" mnemonicParsing="false" onAction="#level2" prefHeight="150.0" prefWidth="150.0" />
                <Button  id="level3" mnemonicParsing="false" onAction="#level3" prefHeight="150.0" prefWidth="150.0" />
            </HBox>
            <HBox alignment="CENTER" spacing="15">
                <Button  id="level4" mnemonicParsing="false" onAction="#level4" prefHeight="150.0" prefWidth="150.0" />
                <Button  id="level5" mnemonicParsing="false"   prefHeight="150.0" prefWidth="150.0" />
                <Button  id="level6" mnemonicParsing="false"   prefHeight="150.0" prefWidth="150.0" />
            </HBox> 
        </VBox>
        
        <!--NeuesLevelButton von Edit-->
        <VBox alignment="CENTER" spacing="20" GridPane.columnIndex="2" GridPane.rowIndex="1" style="-fx-background-color: transparent">
            <HBox alignment="CENTER" spacing="15">
                <Button id="newLevel"  mnemonicParsing="false" onAction="#newLevel" prefHeight="150.0" prefWidth="150.0" />
            </HBox>
        </VBox>
        
        <!--Strich-->
        <Separator blendMode="EXCLUSION" cacheHint="SPEED"  GridPane.columnIndex="1" GridPane.rowIndex="1" orientation="VERTICAL" />

        <!--Beschriftung der Container Edit und Challenge-->
        <HBox alignment="CENTER" spacing="15" GridPane.columnIndex="0" GridPane.rowIndex="2"  >
            <Label text="Challenge" >
                <font>
                    <Font name="Berlin Sans FB Demi Bold" size="30.0" />
                </font>
            </Label>
        </HBox>

        <HBox alignment="CENTER" spacing="15" GridPane.columnIndex="2" GridPane.rowIndex="2" >
            <Label text="Edit" >
                <font>
                    <Font name="Berlin Sans FB Demi Bold" size="30.0" />
                </font>
            </Label>
        </HBox>
     
   </children>
</GridPane>
